<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs442.project.splitpay" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Registration"
            android:label="@string/title_activity_detail_topic"
            android:parentActivityName=".LoginActivity">
        </activity>
        <activity
            android:name=".Groups_Reg"
            android:label="@string/group_name" >
        </activity>
        <activity
            android:name=".Members"
            android:label="@string/group_members" >
        </activity>
        <activity
            android:name=".Create_Group"
            android:label="@string/create_group" >
        </activity>
    </application>

</manifest>
